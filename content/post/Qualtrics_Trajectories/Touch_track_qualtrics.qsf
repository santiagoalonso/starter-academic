ne.addOnload(function()\n{\n\t\/*Place your JavaScript here to run when the page loads*\/\n\n});\n\nQualtrics.SurveyEngine.addOnReady(function()\n{\n\t\/*Place your JavaScript here to run when the page is fully displayed*\/\n\tvar windowWidth = (window.innerWidth)\n\tvar windowHeight = (window.innerHeight)\n\tvar qWidth = windowWidth*0.9\n\tjQuery( \".QuestionText\" ).css(\"width\",  qWidth + \"px\")\n\tjQuery( \"#Buttons\" ).css(\"width\",  qWidth + \"px\");\n});\n\nQualtrics.SurveyEngine.addOnPageSubmit(function(type)\n\/\/everything in this function is executed when Next button is clicked\n{\n\topenFullscreen()\n});\n\nQualtrics.SurveyEngine.addOnUnload(function()\n{\n\t\/*Place your JavaScript here to run when the page is unloaded*\/\n\n});","DefaultChoices":false,"DataExportTag":"Q28","QuestionType":"DB","Selector":"TB","DataVisibility":{"Private":false,"Hidden":false},"Configuration":{"QuestionDescriptionOption":"UseText"},"QuestionDescription":"Luego, mantenga su dedo \u00cdNDICE en el cuadro inferior. NO LO LEVANTE.\u00a0Su tarea es moverse a la pos...","ChoiceOrder":[],"Validation":{"Settings":{"Type":"None"}},"GradingData":[],"Language":[],"NextChoiceId":4,"NextAnswerId":1,"QuestionID":"QID38"}},{"SurveyID":"SV_6VS6k5MiyMoJk0K","Element":"SQ","PrimaryAttribute":"QID41","SecondaryAttribute":"Mu\u00e9vase a la posici\u00f3n ${lm:\/\/Field\/2}. Puede corregir si se coloreo el equivocado. ","TertiaryAttribute":null,"Payload":{"QuestionText":"Mu\u00e9vase a la posici\u00f3n ${lm:\/\/Field\/2}. Puede corregir si se coloreo el equivocado.&nbsp;","QuestionJS":"\/\/Variables and functions defined in this block of code, before addOnload, are available to all blocks of questions, not just this one, and any executable code is executed for every question.\n\n\/\/IMPORTANT:  CHECK ALL QID. THEY CHANGE\n\/\/initialize variables and arrays (all times are in msec)\n\t\n\t\/\/Payment variables\n\n\t\/\/ bookkeeping variables\n\tif (typeof interval == \"undefined\")\t{var interval = 5} \/\/this is the minimun time interval between recordings of cursor position by the function getMousePosition\n\tif (typeof QID_before_slider == \"undefined\")\t{var QID_before_slider = 41} \/\/find them in developer tools of chrome\t\n\tif (typeof QID_after_slider == \"undefined\")\t{var QID_after_slider = 42}  \/\/find them in developer tools of chrome\n\tif (typeof QID_slider == \"undefined\")\t{var QID_slider = \"41-20\"} \/\/find them in developer tools of chrome\n\tif (typeof QID_slider2 == \"undefined\")\t{var QID_slider2 = \"41~20\"} \/\/find them in developer tools of chrome\n\tif (typeof ntrials == \"undefined\")\t{var ntrials = 10} \/\/Should match the loop&merge\n\tif (typeof slider_type == \"undefined\")\t{var slider_type = 'num'} \/\/bar (hit a bar) or num (hit numbers)\n\tif (typeof height_slider == \"undefined\")\t{var height_slider = 50} \/\/pixels;\n\tif (typeof length_slider == \"undefined\")\t{var length_slider = 0} \/\/pixels;\n\tif (typeof start_vert_pos == \"undefined\")\t{var start_vert_pos = .40} \/\/0-1; % of window height\n\n\n\t\/\/variables used to analyze timing of subject's actions on question page\n\tif (typeof loadTime == \"undefined\")\t\t\t\t{var loadTime = [0] }\t\t\/\/absolute time when page begins to load\n\tif (typeof readyTime == \"undefined\")\t\t\t{var readyTime = [0] } \t\t\/\/absolute time when page is fully loaded\n\tif (typeof RT == \"undefined\") \t\t{var RT = [99999999999] }\t\/\/response time: time being in the starting position.\n\tif (typeof MT == \"undefined\")\t{var MT = 99999999999} \/\/movement time: time taken to touch slider after leaving start region\n\tif (typeof time_inside_start_region== \"undefined\")  {var time_inside_start_region = 0 }\n\tif (typeof resp == \"undefined\")\t{var resp =-100000} \/\/slider response\n\tif (typeof slider_pos == \"undefined\")\t{var slider_pos =-100000} \/\/slider position\n\tif (typeof timeFT == \"undefined\")\t{var timeFT =-100000} \/\/time that the slider was touched for the first time (FT) (as in time vector)\n\tif (typeof timeSA == \"undefined\")\t{var timeSA =-100000} \/\/time that the slider appeared (SA) (as in time vector)\n\n\t\/\/variables used to set allowed limits on subject's actions\n\tif (typeof minWindowWidth == \"undefined\")\t\t{var minWindowWidth = 925}\n\tif (typeof minWindowHeight == \"undefined\")\t\t{var minWindowHeight = 675}\t\/\/window dimensions must be large enough to display all buttons wthout scrolling, and will depend on the specific layout of Qualtrics window and size of stimulus images\n    if (typeof ask_win == \"undefined\")\t\t{var ask_win = 0 } \/\/ask if window is of the correct size\n\tif (typeof loop_pos == \"undefined\")\t\t{var loop_pos = 0 } \/\/For training, current position for participant to move to\n\tif (typeof loop_num == \"undefined\")\t\t{var loop_num = 0 }\n\tif (typeof loop_merge == \"undefined\")\t\t{var loop_merge = 0 }\n\tif (typeof lifted_finger == \"undefined\")\t\t{var lifted_finger = false} \t\n\n\t\/\/Boolean variables used to remember if things were done or limits exceeded\n\t{var wait = false}\t\t\t\t\t\t\t\t\/\/is getMousePosition function waiting for its interval to expire? (begins as \"false\" so we don't delay capturing the first mouse move.)\n\t{var outside_start_region = true} \t\t\t\t\t\/\/is the subject outside the starting position \n\t{var slider_appeared = false} \t\t\t\t\t\/\/did the slider appear?\n\t{var inside_slider = false} \t\t\t\t\t\/\/did the subject touched the slider?\n\t{var windowTooSmall = new Boolean(false)} \t\t\/\/was browser window too small?\n\t\/\/isItPractice enables us to distinguish between practice question #0 and real question #0 (because different things must happen)\n\tif (typeof isItPractice == \"undefined\")\t\t\t{var isItPractice = true}\n\n\t\/\/variables used to used to check for correct mouse start position for each question\n\tif (typeof currentXPosition == \"undefined\")\t{var currentXPosition = 0}\n\tif (typeof currentYPosition == \"undefined\")\t\t{var currentYPosition = 0 }\n\tif (typeof currentNextButtonRect == \"undefined\"){var currentNextButtonRect = { left: 0, right: 0, top: 0, bottom: 0} }\n\tif (typeof nextButtonLeft == \"undefined\")\t\t{var nextButtonLeft = [0] }\n\tif (typeof nextButtonRight == \"undefined\")\t\t{var nextButtonRight = [0] }\n\tif (typeof nextButtonTop == \"undefined\")\t\t{var nextButtonTop = [0] }\n\tif (typeof nextButtonBottom == \"undefined\")\t\t{var nextButtonBottom = [0] }\n    if (typeof sliderLeft == \"undefined\")\t\t{var sliderLeft = [0] }\n\tif (typeof sliderRight == \"undefined\")\t\t{var sliderRight = [0] }\n\tif (typeof sliderTop == \"undefined\")\t\t{var sliderTop = [0] }\n\tif (typeof sliderBottom == \"undefined\")\t\t{var sliderBottom = [0] }\n\n\n\t\/\/dimensions of viewport, used to check that browser window is adequately sized\n\tif (typeof windowHeight == \"undefined\" )\t{var windowHeight = [] }\n\tif (typeof windowWidth == \"undefined\" )\t\t{var windowWidth = [] }\n\tif (typeof barWidth == \"undefined\" )\t\t{var barWidth = [] }\n\t\n\t\/\/arrays to record values aquired more than once per question\n\t\n\t\/\/arrays to record vector of mouse X coordinates, Y coordinates and times\n\tif (typeof xPos == \"undefined\") \t{var xPos = [] }\n\tif (typeof yPos == \"undefined\")\t\t{var yPos = [] }\n\tif (typeof time == \"undefined\")\t\t{var time = [] }\n\t\/\/array to record which alerts happened\n\tif (typeof alerts == \"undefined\" )\t{var alerts = [] }\n    \n\n\n\n\/\/define functions\n\n\t\n\tfunction coordinates_next_button(){\n\t\t\/\/next button coordinates. the next button is also the start position\n\t\tcurrentNextButtonRect = document.getElementById(\"NextButton\").getBoundingClientRect()\n\t\tnextButtonLeft[ 0 ] = Math.round(currentNextButtonRect.left)\n\t\tnextButtonRight[ 0 ] = Math.round(currentNextButtonRect.right)\n\t\tnextButtonTop[ 0 ] = Math.round(currentNextButtonRect.top)\n\t\tnextButtonBottom[0 ] = Math.round(currentNextButtonRect.bottom)\n\t}\t\n\n\tfunction coordinates_slider(){\n\t\t\/\/slider coordinates\n\t\tif (slider_type=='bar'){\n\t\t\tcurrentSliderRect = document.getElementsByClassName(\"slider-container\")\n\t\t} else if (slider_type =='num'){\n\t\t\tcurrentSliderRect = document.getElementsByClassName(\"labels-container\")\n\t\t}\n\t\t\n\t\tcurrentSliderRect = currentSliderRect[0].getBoundingClientRect() \/\/assuming there is only one slider and the first one is the target (.getElementsByClassName puts all in one list)\n\t\tsliderLeft[0 ] = Math.round(currentSliderRect.left)\n\t\tsliderRight[ 0 ] = Math.round(currentSliderRect.right)\n\t\tsliderTop[0 ] = Math.round(currentSliderRect.top)\n\t\tsliderBottom[ 0 ] = Math.round(currentSliderRect.bottom)\n\t}\n\n\tfunction color_slider(bg_ele_col, slider_class){\n\t\t\/\/paints the slider with a color\n\t\t\/\/bg_ele_col: background or element color (string)\n\t\t\/\/slider_class: (string) e.g. track QID40-20-track; QID of slider can change; check it in developer tools of browse\n\t\t\n\t\tdocument.getElementsByClassName(\"labels-container\")[0].style.color = bg_ele_col\n\t\tdocument.getElementsByClassName(slider_class)[0].style.borderColor  = bg_ele_col \n\t\tdocument.getElementsByClassName('bar')[0].style.background  = bg_ele_col \n\t\t\n\t}\n\n\tfunction update_slider(slider_class, slider_updated_pos){\n\t\t\/\/slider_class: (string) e.g. track QID40-20-track; QID of slider can change; check it in developer tools of browse\n\t\t\/\/slider_updated_pos: (string) e.g. '44px'\n\t\t\n\t\t\n\t\tif (slider_type=='bar'){\n\t\t\tjQuery( slider_class ).css(\"width\", slider_updated_pos);\t\n\t\t} else if (slider_type =='num') {\n\t\t\tvar lis = document.getElementsByClassName(\"numbers\")[0].getElementsByTagName(\"li\");\n\t\t\tvar COORD = lis[0].getBoundingClientRect() \/\/initialize\n\t\t\tfor (let i = 0; i < lis.length; i++) {\n\t\t\t  \t\tCOORD = lis[i].getBoundingClientRect()\n\t\t\t\t\tlis[i].style.color = 'black' \/\/default color of numbers\n\t\t\t\t\tif ((xPos[xPos.length-1]  >  (COORD.left) ) &\n\t\t\t\t\t\t(xPos[xPos.length-1]  <  (COORD.right) ) &\n\t\t\t\t\t\t(yPos[yPos.length-1]  >  (COORD.top) ) &\n\t\t\t\t\t\t(yPos[yPos.length-1]  <  (COORD.bottom)))\n\t\t\t\t\t{ \/\/color the number where the mouse\/finger landed\n\t\t\t\t\t\tlis[i].style.color = 'aqua' \n\t\t\t\t\t\tresp = i\n\t\t\t\t\t}\t\t\n\t\t\t}\t\n\t\t}\n\t\t\n\t}\n\n\tfunction touchEnd(event){\n\t\t\n\t\tif (slider_appeared & inside_slider){\n\t\t\tupdate_slider('.bar', slider_pos + \"px\")\n\t\t\tdocument.removeEventListener(\"touchend\", touchEnd)\n\t\t} else if ((slider_appeared) & (!lifted_finger)){\n\t\t\t\/\/detects finger jumps\n\t\t\tlifted_finger = true\n\t\t    alert(\"No levante el dedo mientras se mueve de la posici\u00f3n inicial a la barra horizontal.\\nDesplace el dedo sobre la pantalla.\\n\\nEn cualquier otro momento puede levantarlo, pero no mientras se mueve del rectangulo central a la barra superior\")\t\t\n\t\t}\n\t\t\n\t}\n\n\tfunction inside_start(event){\n\t\toutside_start_region = false\n\t\t\/\/inside start region\n\t\tcolor_slider('black', \"track \" + loop_merge + \"_QID\" + QID_slider + \"-track\")\n\t\tslider_appeared = true\n\t\tjQuery( \"#NextButton\" ).prop( \"disabled\", false );\n\t}\n\n\t\/\/define the function that gets and records the mouse position vector as fast as the processor can do it (approx 10-20 msec in this code)\n\tfunction getMousePosition(event) \n\t{\/\/begin: getMousePosition function\n\n\t\tcoordinates_next_button()\n\t\tcoordinates_slider()\n\t\tif (!wait)\n\t\t{\n\t\t\twait=true\n\t\t\t\/\/ record the time of movement\n\t\t\tvar currentTime = Date.now()\n\t\t\ttime.push(currentTime - readyTime[0])\n\t\t\t\/\/ record mouse coordinates relative to upper left corner of browser window\n\t\t\tconsole.log(event.type)\n\t\t\tif(event.type == 'touchstart' || event.type == 'touchmove' || event.type == 'touchend' || event.type == 'touchcancel'){\n\t\t\t\tvar evt = (typeof event.originalEvent === 'undefined') ? event : event.originalEvent;\n   \t\t\t\tvar touch = evt.touches[0] || evt.changedTouches[0];\n\t\t\t\tvar x =  touch.clientX\n\t\t\t\tvar y =  touch.clientY\n\t\t\t\t\n\t\t\t} else {\n\t\t\t\tvar x =  event.clientX\n\t\t\t\tvar y =  event.clientY\n\t\t\t}\n\t\t\t\n\t\t\tcurrentXPosition = Math.round( x ) \/\/Math.round( event.clientX )\n\t\t\tcurrentYPosition = Math.round( y ) \/\/Math.round( event.clientY )\n\t\t\txPos.push(currentXPosition)\n\t\t\tyPos.push(currentYPosition)\n\t\t\t\/\/wait an interval before allowing getMousePosition to run again\n\t\t\tsetTimeout(() => wait = false, interval)\n\t\t\t\n\t\t\t\n   \t\t \t\n\t\t\t\n\t\t\t\n\t\t\tif\t(\t(currentXPosition  <  ( nextButtonLeft[0] ) ) ||\n\t\t\t\t(currentXPosition  >  ( nextButtonRight[0]) ) ||\n\t\t\t\t(currentYPosition  >  ( nextButtonBottom[0] ) ) ||\n\t\t\t\t(currentYPosition  <  ( nextButtonTop[0] ) )\n\t\t\t)\n\t\t\t{\t\n\t\t\t\toutside_start_region = true\n\t\t\t} else {\n\t\t\t    outside_start_region = false\n\t\t\t}\n\t\t\t\n\t\t\tvar lis = document.getElementsByClassName(\"numbers\")[0].getElementsByTagName(\"li\");\n\t\t\tvar max_val_slider = parseInt(lis[lis.length-1].innerHTML) \n\t\t\tif ((!slider_appeared) & (!outside_start_region)) { \/\/inside start region make bar appear\n\t\t\t\t\/\/jQuery( \"sliderGrid\" ).show();\n\t\t\t\tcolor_slider('black', \"track \" + loop_merge + \"_QID\" + QID_slider + \"-track\")\n\t\t\t\tif (slider_type=='num'){\n\t\t\t\t\tjQuery(\"li\").css({ \"border\": \"1px solid black\"})\n\t\t\t\t}\n\t\t\t\tslider_appeared = true\n\t\t\t\ttime_inside_start_region = Date.now()\n\t\t\t\ttimeSA = time[time.length-1]\n\t\t\t\tQualtrics.SurveyEngine.setEmbeddedData(\"time_slider_appeared_train_\" + parseInt(\"${lm:\/\/CurrentLoopNumber}\"), timeSA)\n\t\t\t} \n\t\t\t\n\t\t\tif ((RT[0] == 0) & slider_appeared & outside_start_region){\n\t\t\t\tRT[0] = Date.now() - time_inside_start_region\n\t\t\t\tQualtrics.SurveyEngine.setEmbeddedData(\"rt_train_\"+ parseInt(\"${lm:\/\/CurrentLoopNumber}\"), RT[0])\n\t\t\t}\n\t\t\t\n\t\t\tif ( slider_appeared &\t\n\t\t\t\t(currentXPosition  >  ( sliderLeft[0]) ) &\n\t\t\t\t(currentXPosition  <  ( sliderRight[0]) ) &\n\t\t\t\t(currentYPosition  >  ( sliderTop[0]) ) &\n\t\t\t\t(currentYPosition  <  ( sliderBottom[0]) )\n\t\t\t)\n\t\t\t{\/\/begin: if slider block \n\t\t\t\t\/\/turn off mousetracking when inside slider\n\t\t\t\tif (!inside_slider){\n\t\t\t\t\ttimeFT = time[time.length-1]\n\t\t\t\t\tMT = (currentTime-time_inside_start_region) - RT[0]\n\t\t\t\t\tQualtrics.SurveyEngine.setEmbeddedData(\"time_slider_touch_train_\" + parseInt(\"${lm:\/\/CurrentLoopNumber}\"), timeFT)\n\t\t\t\t\tQualtrics.SurveyEngine.setEmbeddedData(\"mt_train_\" + parseInt(\"${lm:\/\/CurrentLoopNumber}\"), MT)\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\t\n\t\t\t\t\n\t\t\t\t\n\t\t\t\t\/\/Enable the next button\n\t\t\t\tjQuery( \"#NextButton\" ).prop( \"disabled\", false );\n\t\t\t\t\n\t\t\t\t\n\t\t\t\t\/\/get and record Next button location (to determine valid starting location for the next question\n\t\t\t\tcoordinates_next_button()\n\t\t\t\tcoordinates_slider()\n\t\t\t\t\n\t\t\t\t\n\t\t\t\t\/\/update slider\n\t\t\t\tlength_slider = sliderRight[0] - sliderLeft[0]\n\t\t\t\tslider_pos = Math.max(0, currentXPosition - sliderLeft[0])\n\t\t\t\tresp = slider_pos\/length_slider * max_val_slider\n\t\t\t\tif (slider_type=='bar'){\n\t\t\t\t\tupdate_slider('.bar', slider_pos + \"px\")\t\n\t\t\t\t} else if (slider_type =='num') {\n\t\t\t\t\tupdate_slider(' ', resp) \/\/NOTE: this one also changes resp.\n\t\t\t\t}\n\t\t\t\t\t\t\t\n\t\t\t\tQualtrics.SurveyEngine.setEmbeddedData(\"resp_train_\" + parseInt(\"${lm:\/\/CurrentLoopNumber}\"), resp)\n\t\t\t\t\n\t\t\t\t\n\n\n\t\t\t\t\/\/check window size, just in case subject re-sized window\n\t\t\t\tif  ( (window.innerWidth < minWindowWidth) || (window.innerHeight < minWindowHeight) ) {windowTooSmall = true}\n\t\t\t\t\n\t\t\t\t\n\t\t\t\t\/\/evoke function to i